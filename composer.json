{
    "name": "lanser/license-mgr",
    "description": "Basic license manager library for Laravel-based projects",
    "keywords": ["license","manager","License Manager","license-mgr","laravel"],
    "homepage": "https://github.com/lanser/license-mgr",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Lanser",
            "email": "martinlanser@gmail.com",
            "homepage": "http://www.martinlanser.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Lanser\\LicenseManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lanser\\LicenseManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
